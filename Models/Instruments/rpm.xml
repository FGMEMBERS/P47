<?xml version="1.0"?>
<PropertyList>

    <path>rpm.ac</path>

    <animation>
        <type>material</type>
        <object-name>RPM.face</object-name>
        <object-name>RPM.needle</object-name>
        <emission>
            <red>0.8</red>
            <green>0.5</green>
            <blue>0.3</blue>
            <factor-prop>/systems/electrical/outputs/instrument-lights-norm</factor-prop>
        </emission>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>RPM.needle</object-name>
        <property>engines/engine/rpm</property>
        <interpolation>
            <entry><ind>0.0</ind><dep>0</dep></entry>
            <entry><ind>1500</ind><dep>110</dep></entry>
            <entry><ind>3000</ind><dep>220</dep></entry>
            <entry><ind>4500</ind><dep>330</dep></entry>
        </interpolation>
        <axis>
            <x>-1.0</x>
            <y>0.0</y>
            <z>0.0</z>
        </axis>
    </animation>

</PropertyList>