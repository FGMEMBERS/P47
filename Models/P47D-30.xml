<?xml version="1.0"?>

<PropertyList>

    <path>p47d-30.ac</path>

    <animation>
        <type>shader</type>
        <shader>chrome</shader>
        <texture>glass.png</texture>
        <object-name>Canopy</object-name>
        <object-name>glass</object-name>
    </animation>




<!--    SURFACE POSITIONS-->

    <animation>
        <type>rotate</type>
        <object-name>rudder</object-name>
        <property>controls/flight/rudder</property>
        <factor>28</factor>
        <center>
            <x-m>5.52</x-m>
            <y-m>0</y-m>
            <z-m>0.793</z-m>
        </center>
        <axis>
            <x>0</x>
            <y>0</y>
            <z>1</z>
        </axis>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>elevator</object-name>
        <property>controls/flight/elevator</property>
        <factor>25</factor><!--30 up 20 down-->
        <center>
            <x-m>5.445</x-m>
            <y-m>0</y-m>
            <z-m>0.192</z-m>
        </center>
        <axis>
            <x>0</x>
            <y>1</y>
            <z>0</z>
        </axis>
    </animation>

<!-- LH Gear-->

    <animation>
        <type>rotate</type>
        <object-name>LGdoorI</object-name>
        <property>gear/gear[0]/position-norm</property>
        <interpolation>
            <entry>
                <ind>0.0</ind><dep>0.0</dep>
            </entry>
            <entry>
                <ind>0.1</ind><dep>-90.0</dep>
            </entry>
            <entry>
                <ind>1.0</ind><dep>-90.0</dep>
            </entry>
        </interpolation>
        <center>
            <x-m>-0.908</x-m>
            <y-m>-0.611</y-m>
            <z-m>-0.754</z-m>
        </center>
        <axis>
            <x>-0.999</x>
            <y>-0.031</y>
            <z>0.022</z>
        </axis>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>LGdoorO</object-name>
        <object-name>Lstrut</object-name>
        <object-name>Lshock</object-name>
        <object-name>LLdamper</object-name>
        <object-name>LUdamper</object-name>
        <object-name>Ltire</object-name>
        <property>gear/gear[0]/position-norm</property>
        <interpolation>
            <entry>
                <ind>0.0</ind><dep>85.0</dep>
            </entry>
            <entry>
                <ind>0.1</ind><dep>85.0</dep>
            </entry>
            <entry>
                <ind>1.0</ind><dep>0.0</dep>
            </entry>
        </interpolation>
        <center>
            <x-m>-1.52</x-m>
            <y-m>-2.648</y-m>
            <z-m>-0.522</z-m>
        </center>
        <axis>
            <x>1</x>
            <y>0.0</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>LGdoorO</object-name>
        <object-name>Lstrut</object-name>
        <object-name>Lshock</object-name>
        <object-name>LLdamper</object-name>
        <object-name>LUdamper</object-name>
        <object-name>Ltire</object-name>
        <property>gear/gear[0]/position-norm</property>
        <interpolation>
            <entry>
                <ind>0.0</ind><dep>20.0</dep>
            </entry>
            <entry>
                <ind>0.1</ind><dep>20.0</dep>
            </entry>
            <entry>
                <ind>1.0</ind><dep>0.0</dep>
            </entry>
        </interpolation>
        <center>
            <x-m>-1.52</x-m>
            <y-m>-2.648</y-m>
            <z-m>-0.522</z-m>
        </center>
        <axis>
            <x>0</x>
            <y>-1.0</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>spin</type>
        <object-name>Ltire</object-name>
        <property>gear/gear[0]/tire-rpm</property>
        <center>
            <x-m>-1.799</x-m>
            <y-m>-2.432</y-m>
            <z-m>-2.096</z-m>
        </center>
        <axis>
            <x>0.0</x>
            <y>-1</y>
            <z>0</z>
        </axis>
    </animation>

