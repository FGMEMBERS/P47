<!-- Prop-->

    <animation>
        <type>select</type>
        <object-name>blade1</object-name>
        <object-name>blade2</object-name>
        <object-name>blade3</object-name>
        <object-name>blade4</object-name>
        <condition>
            <less-than>
                <property>engines/engine[0]/rpm</property>
                <value>500.000</value>
            </less-than>
        </condition>
    </animation>

    <animation>
        <type>select</type>
        <object-name>propblur</object-name>
        <condition>
            <and>
                <greater-than>
                    <property>engines/engine[0]/rpm</property>
                    <value>499.999</value>
                </greater-than>
                <less-than>
                    <property>engines/engine[0]/rpm</property>
                    <value>1000.000</value>
                </less-than>
            </and>
        </condition>
    </animation>

    <animation>
        <type>select</type>
        <object-name>propdisk</object-name>
        <condition>
            <greater-than>
                <property>engines/engine[0]/rpm</property>
                <value>999.999</value>
            </greater-than>
        </condition>
    </animation>

    <animation>
        <type>spin</type>
        <object-name>blade1</object-name>
        <object-name>blade2</object-name>
        <object-name>blade3</object-name>
        <object-name>blade4</object-name>
        <property>engines/engine/rpm</property>
        <center>
            <x-m>-4.354</x-m>
            <y-m>0</y-m>
            <z-m>-0.048</z-m>
        </center>
        <axis>
            <x>-1.0</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>blade1</object-name>
